<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="SastImgClient.Pages.Video.VideoPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="using:Windows.Media.Core"
    xmlns:core="using:SastImgClient.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:SastImgClient.Pages.Video"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <Page.Resources>
        <DataTemplate x:Key="PlayListTemplate" x:DataType="core:VideoItem">
            <TextBlock Text="{x:Bind Title}" />
        </DataTemplate>
    </Page.Resources>

    <Grid>
        <StackPanel Orientation="Horizontal">

        </StackPanel>
    </Grid>
</Page>
